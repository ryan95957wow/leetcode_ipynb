    "def binary_search(li, val):\n",
    "    l,r=0,len(li)-1\n",
    "    while l<=r:\n",
    "        mid = l+(l+r)//2\n",
    "        if li[mid]>val:\n",
    "            r = mid-1\n",
    "        elif li[mid]<val:\n",
    "            l = mid+1\n",
    "        elif li[mid]==val:\n",
    "            return mid\n",
    "        else:\n",
    "            return None"
   ]
  }
